  },
  "devDependencies": {
    "electron": "^7.3.2",
    "electron-builder": "^22.8.1",
    "standard": "^14.3.4"
  },
  "build": {
    "mac": {
      "darkModeSupport": true
    }
  },
  "standard": {
    "ignore": [
      "/icomoon/",
      "/branches/",
      "/third-party/",
      "raw-viewer.component.js",
      "/build/"
